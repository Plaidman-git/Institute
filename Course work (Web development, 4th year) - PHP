#trains.php
        
<?php
/* Установка локали и даты */
setlocale(LC_ALL, "russian");  // получить имя месяца по-русски (LC_ALL, "russian" - все нижеперечисленное по-русски / кодировка будет 1251)

  /*Инициализация многомерного массива: */
$kursMenuTables = [
	['link'=>'Билеты', 'href'=>'tickets.php'],
	['link'=>'Покупатели', 'href'=>'buyers.php'],
	['link'=>'Качество', 'href'=>'quality.php'],
	['link'=>'Время', 'href'=>'time.php'],
	['link'=>'Сотрудники', 'href'=>'staff.php'],	
];

$kursMenuAdmin = [
	['link'=>'phpMyAdmin', 'href'=>'http://localhost/phpmyadmin'],
	['link'=>'Создать и заполнить таблицу', 'href'=>'http://localhost/phpmyadmin/index.php?route=/database/structure&db=trains'],
	['link'=>'Редактор таблицы', 'href'=>'http://localhost/phpmyadmin/index.php?route=/table/create&db=trains'],
];
?>


<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="utf-8" />
  <title>Курсовая работа</title>
 </head>

 <body>
 <div class="flex-container">

  <header class="flex-item item-1">
	<center>
     <font face="Arial">Меню курсовой работы</font>
	</center>
  </header>

  <main>
   <section class="flex-item item-2">
	<center><big><b>Справочники</b></big></center>
      <?php
         foreach ($kursMenuTables as $item) 
	    { // $item - внутренний массив
		echo "<li>";
			echo "<a href='{$item['href']}'> {$item['link']} </a>"; 
		echo "</li>";	
	    }
      ?>
   </section>

<section class="flex-item item-5">
	<center><big><b>Администрирование</b></big></center>
     <?php
         foreach ($kursMenuAdmin as $item) 
	    { // $item - внутренний массив
		echo "<li>";
			echo "<a href='{$item['href']}'> {$item['link']} </a>"; 
		echo "</li>";	
	    }
      ?>	
   </section>

  </main>
   <footer class="flex-item item-6"></footer>
 </div>
 </body>
</html>

