foreach ($tickets as $sub_array) {
    echo $sub_array['Дата отправления'];
    echo $sub_array['Время отправления'];
    echo $sub_array['Дата прибытия'];
    echo $sub_array['Время прибытия'];
    echo $sub_array['Стоимость билета'];
    echo $sub_array['Покупатель'];
    echo $sub_array['Пункт отправления'];
    echo $sub_array['Пункт прибытия'];
    echo "<br>";
}

